{
  "name": "augmentedjs-next",
  "version": "3.0.0",
  "description": "The Augmented.js Next Framework.",
  "main": "dist/augmented-next.js",
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "test": "mocha --require @babel/register --require test/helper.js -c test/*Spec.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/augmented-next.git"
  },
  "keywords": [
    "augmentedjs"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "foodoc": "0.0.9",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jsdoc": "^3.6.4",
    "jsdom": "^16.2.2",
    "mocha": "^7.2.0",
    "mocha-jsdom": "^2.0.0",
    "sinon": "^9.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "next-core-application": "^2.5.4",
    "next-core-bundle": "^2.5.3",
    "next-core-logger": "^2.8.1",
    "next-core-model": "^3.1.3",
    "next-core-object": "^3.0.0",
    "next-core-security": "^2.5.3",
    "next-core-sort": "^2.2.0",
    "next-core-structures": "^2.6.0",
    "next-core-utilities": "^2.9.0",
    "next-core-validation": "^2.5.5"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/augmented-next/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
