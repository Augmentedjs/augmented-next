{
  "name": "augmentedjs-next",
  "version": "2.6.0",
  "description": "The Augmented.js Next Framework.",
  "main": "dist/augmented-next.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader",
    "build": "webpack --mode production --module-bind js=babel-loader",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/augmented-next.git"
  },
  "keywords": [
    "augmentedjs"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "foodoc": "0.0.9",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.2",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "mocha-jsdom": "^2.0.0",
    "sinon": "^7.3.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "next-core-application": "^2.5.3",
    "next-core-bundle": "^2.5.3",
    "next-core-logger": "^2.5.3",
    "next-core-model": "^2.6.1",
    "next-core-object": "^2.6.0",
    "next-core-security": "^2.5.3",
    "next-core-sort": "^2.0.0",
    "next-core-structures": "^2.5.3",
    "next-core-utilities": "^2.6.1",
    "next-core-validation": "^2.5.4"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/augmented-next/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
