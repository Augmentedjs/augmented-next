{
  "name": "augmentedjs-next",
  "version": "2.1.0",
  "description": "The Augmented.js Next Framework.",
  "main": "dist/augmented-next.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader",
    "build": "webpack --mode production --module-bind js=babel-loader",
    "lint": "eslint ./src/augmented.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "docs": "jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/augmented-next.git"
  },
  "keywords": [
    "augmentedjs"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "express": "^4.16.3",
    "foodoc": "0.0.9",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.12.0",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.2.0",
    "sinon": "^4.5.0",
    "webpack": "^4.16.4",
    "webpack-cli": "^2.1.5",
    "lodash.bind": "^4.2.1",
    "lodash.clone": "^4.5.0",
    "lodash.defaults": "^4.2.0",
    "lodash.defer": "^4.1.0",
    "lodash.escape": "^4.0.1",
    "lodash.has": "^4.5.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.iteratee": "^4.7.0",
    "lodash.result": "^4.5.2"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/Augmentedjs/augmented-next/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
