{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "systemSummary": "Augmented.js Next - Simply Development.",
    "systemColor": "#607D8B",
    "systemLogo": "images/jensen-silo-circle-small.png",
    "logoFile": "images/jensen-silo-circle-small.png",
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Augmented.js",
    "footer": "<a href='http://www.augmentedjs.com'>Augmented.js Homepage</a>",
    "copyright": "",
    "navType": "vertical",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "html://",
    "methodHeadingReturns": false,
    "favicon": "images/favicon-32x32.png",
    "showTableOfContents": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include": [
      "./node_modules/next-core-application/src",
      "./node_modules/next-core-bundle/src",
      "./node_modules/next-core-logger/src",
      "./node_modules/next-core-model/src",
      "./node_modules/next-core-object/src",
      "./node_modules/next-core-security/src",
      "./node_modules/next-core-sort/src",
      "./node_modules/next-core-structures/src",
      "./node_modules/next-core-utilities/src",
      "./node_modules/next-core-validation/src"
    ],
    "exclude": [
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  }
}
